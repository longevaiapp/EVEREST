generator client {
  provider = "prisma-client-js"
  output   = "../backend/node_modules/.prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id            String             @id
  petId         String
  fecha         DateTime           @db.Date
  hora          String
  tipo          Appointment_tipo
  motivo        String             @db.Text
  confirmada    Boolean            @default(false)
  cancelada     Boolean            @default(false)
  notas         String?            @db.Text
  createdById   String?
  createdAt     DateTime           @default(now())
  updatedAt     DateTime
  status        Appointment_status @default(PENDIENTE)
  visitId       String?
  agendadaPorId String?
  creadaPor     String?
  labRequestId  String?
  metadata      String?            @db.LongText
  Pet           Pet                @relation(fields: [petId], references: [id])

  @@index([creadaPor])
  @@index([fecha])
  @@index([petId])
  @@index([status])
}

model Consultation {
  id                   String              @id
  visitId              String              @unique
  petId                String
  doctorId             String
  startTime            DateTime            @default(now())
  endTime              DateTime?
  symptoms             String?             @db.Text
  physicalExam         String?             @db.Text
  diagnosis            String?             @db.Text
  treatment            String?             @db.Text
  notes                String?             @db.Text
  vitalTemperature     Float?
  vitalHeartRate       Int?
  vitalRespiratoryRate Int?
  vitalWeight          Float?
  followUpRequired     Boolean             @default(false)
  followUpDate         DateTime?
  status               Consultation_status @default(EN_PROGRESO)
  createdAt            DateTime            @default(now())
  updatedAt            DateTime
  duration             Int?
  followUpNotes        String?             @db.Text
  historiaEnfermedad   String?             @db.Text
  motivoConsulta       String?             @db.Text
  soapAssessment       String?             @db.Text
  soapObjective        String?             @db.Text
  soapPlan             String?             @db.Text
  soapSubjective       String?             @db.Text
  vitalBloodPressure   String?
  vitalHydration       String?
  vitalMucosas         String?
  vitalPulse           Int?
  User                 User                @relation(fields: [doctorId], references: [id])
  Pet                  Pet                 @relation(fields: [petId], references: [id])
  Visit                Visit               @relation(fields: [visitId], references: [id])
  Diagnostico          Diagnostico[]
  Hospitalization      Hospitalization?
  LabRequest           LabRequest[]
  Prescription         Prescription[]
  SignosVitales        SignosVitales[]
  Surgery              Surgery[]

  @@index([doctorId])
  @@index([petId])
  @@index([startTime])
  @@index([status])
}

model Diagnostico {
  id             String                 @id
  consultationId String
  codigoCIE10    String?
  nombreCIE10    String?
  descripcion    String                 @db.Text
  tipo           Diagnostico_tipo       @default(PRESUNTIVO)
  severidad      Diagnostico_severidad?
  observaciones  String?                @db.Text
  diferenciales  String?                @db.Text
  orden          Int                    @default(1)
  esPrincipal    Boolean                @default(false)
  createdAt      DateTime               @default(now())
  updatedAt      DateTime
  Consultation   Consultation           @relation(fields: [consultationId], references: [id], onDelete: Cascade)

  @@index([codigoCIE10])
  @@index([consultationId])
}

model Dispense {
  id             String          @id
  prescriptionId String
  petId          String
  dispensedById  String
  dispensedAt    DateTime        @default(now())
  status         Dispense_status @default(PENDIENTE)
  notes          String?         @db.Text
  deliveredTo    String?
  signature      String?         @db.LongText
  createdAt      DateTime        @default(now())
  User           User            @relation(fields: [dispensedById], references: [id])
  Pet            Pet             @relation(fields: [petId], references: [id])
  Prescription   Prescription    @relation(fields: [prescriptionId], references: [id])
  DispenseItem   DispenseItem[]

  @@index([dispensedById], map: "Dispense_dispensedById_fkey")
  @@index([petId])
  @@index([prescriptionId])
  @@index([status])
}

model DispenseItem {
  id             String     @id
  dispenseId     String
  medicationId   String
  medicationName String
  requestedQty   Int
  dispensedQty   Int
  reason         String?
  unitPrice      Decimal    @db.Decimal(10, 2)
  subtotal       Decimal    @db.Decimal(10, 2)
  createdAt      DateTime   @default(now())
  Dispense       Dispense   @relation(fields: [dispenseId], references: [id], onDelete: Cascade)
  Medication     Medication @relation(fields: [medicationId], references: [id])

  @@index([dispenseId])
  @@index([medicationId])
}

model Hospitalization {
  id                  String                 @id
  petId               String
  consultationId      String                 @unique
  surgeryId           String?                @unique
  admittedById        String
  admittedAt          DateTime               @default(now())
  dischargedAt        DateTime?
  reason              String                 @db.Text
  location            String?
  status              Hospitalization_status @default(ACTIVA)
  createdAt           DateTime               @default(now())
  updatedAt           DateTime
  cuidadosEspeciales  String?                @db.Text
  dischargeNotes      String?                @db.Text
  estimacionDias      Int?
  frecuenciaMonitoreo String?
  User                User                   @relation(fields: [admittedById], references: [id])
  Consultation        Consultation           @relation(fields: [consultationId], references: [id])
  Pet                 Pet                    @relation(fields: [petId], references: [id])
  Surgery             Surgery?               @relation(fields: [surgeryId], references: [id])
  Monitoring          Monitoring[]

  @@index([admittedById], map: "Hospitalization_admittedById_fkey")
  @@index([petId])
  @@index([status])
}

model LabRequest {
  id                                  String             @id
  consultationId                      String
  petId                               String
  requestedById                       String
  requestedAt                         DateTime           @default(now())
  type                                LabRequest_type
  urgency                             LabRequest_urgency @default(NORMAL)
  notes                               String?            @db.Text
  status                              LabRequest_status  @default(PENDIENTE)
  results                             String?            @db.Text
  resultFiles                         String?            @db.LongText
  completedAt                         DateTime?
  completedById                       String?
  createdAt                           DateTime           @default(now())
  updatedAt                           DateTime
  User_LabRequest_completedByIdToUser User?              @relation("LabRequest_completedByIdToUser", fields: [completedById], references: [id])
  Consultation                        Consultation       @relation(fields: [consultationId], references: [id])
  Pet                                 Pet                @relation(fields: [petId], references: [id])
  User_LabRequest_requestedByIdToUser User               @relation("LabRequest_requestedByIdToUser", fields: [requestedById], references: [id])

  @@index([completedById], map: "LabRequest_completedById_fkey")
  @@index([consultationId])
  @@index([petId])
  @@index([requestedById], map: "LabRequest_requestedById_fkey")
  @@index([status])
}

model MedicalNote {
  id             String           @id
  petId          String
  consultationId String?
  createdById    String
  type           MedicalNote_type
  content        String           @db.Text
  isPrivate      Boolean          @default(false)
  createdAt      DateTime         @default(now())
  updatedAt      DateTime
  User           User             @relation(fields: [createdById], references: [id])
  Pet            Pet              @relation(fields: [petId], references: [id])

  @@index([createdById], map: "MedicalNote_createdById_fkey")
  @@index([petId])
  @@index([type])
}

model Medication {
  id                    String              @id
  name                  String
  genericName           String?
  category              Medication_category
  presentation          String
  concentration         String?
  unit                  String
  currentStock          Int                 @default(0)
  minStock              Int
  maxStock              Int?
  location              String?
  requiresRefrigeration Boolean             @default(false)
  isControlled          Boolean             @default(false)
  costPrice             Decimal?            @db.Decimal(10, 2)
  salePrice             Decimal             @db.Decimal(10, 2)
  expirationDate        DateTime?
  lastRestocked         DateTime?
  supplier              String?
  supplierCode          String?
  activo                Boolean             @default(true)
  createdAt             DateTime            @default(now())
  updatedAt             DateTime
  DispenseItem          DispenseItem[]
  PurchaseOrderItem     PurchaseOrderItem[]
  StockAlert            StockAlert[]
  StockMovement         StockMovement[]

  @@index([category])
  @@index([currentStock])
  @@index([name])
}

model Monitoring {
  id                     String                      @id
  hospitalizationId      String
  recordedById           String
  recordedAt             DateTime                    @default(now())
  temperatura            Float?
  frecuenciaCardiaca     Int?
  frecuenciaRespiratoria Int?
  presionArterial        String?
  nivelConciencia        Monitoring_nivelConciencia?
  escalaDolor            Int?
  observaciones          String?                     @db.Text
  createdAt              DateTime                    @default(now())
  Hospitalization        Hospitalization             @relation(fields: [hospitalizationId], references: [id], onDelete: Cascade)
  User                   User                        @relation(fields: [recordedById], references: [id])

  @@index([hospitalizationId])
  @@index([recordedAt])
  @@index([recordedById], map: "Monitoring_recordedById_fkey")
}

model Notification {
  id        String            @id
  userId    String
  tipo      Notification_tipo
  titulo    String
  mensaje   String            @db.Text
  leida     Boolean           @default(false)
  data      String?           @db.LongText
  createdAt DateTime          @default(now())
  User      User              @relation(fields: [userId], references: [id])

  @@index([createdAt])
  @@index([userId, leida])
}

model Owner {
  id           String   @id
  nombre       String
  telefono     String   @unique
  email        String?
  direccion    String?  @db.Text
  ciudad       String?
  codigoPostal String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime
  Pet          Pet[]

  @@index([nombre])
  @@index([telefono])
}

model Payment {
  id          String             @id
  visitId     String             @unique
  total       Decimal            @db.Decimal(10, 2)
  metodoPago  Payment_metodoPago
  fecha       DateTime           @default(now())
  recibidoPor String?
  notas       String?            @db.Text
  createdAt   DateTime           @default(now())
  Visit       Visit              @relation(fields: [visitId], references: [id])

  @@index([fecha])
}

model Pet {
  id                       String                 @id
  numeroFicha              String                 @unique
  ownerId                  String
  nombre                   String
  especie                  Pet_especie
  raza                     String?
  sexo                     Pet_sexo
  fechaNacimiento          DateTime?
  peso                     Float?
  color                    String?
  condicionCorporal        Pet_condicionCorporal?
  fotoUrl                  String?                @db.LongText
  snapTest                 String?                @db.Text
  analisisClinicos         String?                @db.Text
  antecedentes             String?                @db.Text
  desparasitacionExterna   Boolean?
  ultimaDesparasitacion    DateTime?
  vacunasTexto             String?                @db.Text
  vacunasActualizadas      Boolean?
  ultimaVacuna             DateTime?
  esterilizado             Boolean?
  otrasCirugias            Boolean?
  detalleCirugias          String?                @db.Text
  ultimoCelo               DateTime?
  cantidadPartos           Int?
  ultimoParto              DateTime?
  alimento                 String?
  porcionesPorDia          String?
  otrosAlimentos           String?
  frecuenciaOtrosAlimentos String?
  alergias                 String?                @db.Text
  enfermedadesCronicas     String?                @db.Text
  conviveOtrasMascotas     Boolean?
  cualesMascotas           String?
  actividadFisica          Boolean?
  frecuenciaActividad      String?
  saleViaPublica           Boolean?
  frecuenciaSalida         String?
  otrosDatos               String?                @db.Text
  estado                   Pet_estado             @default(RECIEN_LLEGADO)
  fechaIngreso             DateTime               @default(now())
  primeraVisita            Boolean                @default(true)
  activo                   Boolean                @default(true)
  createdAt                DateTime               @default(now())
  updatedAt                DateTime
  Appointment              Appointment[]
  Consultation             Consultation[]
  Dispense                 Dispense[]
  Hospitalization          Hospitalization[]
  LabRequest               LabRequest[]
  MedicalNote              MedicalNote[]
  Owner                    Owner                  @relation(fields: [ownerId], references: [id], onDelete: Cascade)
  Prescription             Prescription[]
  Surgery                  Surgery[]
  Task                     Task[]
  VaccineRecord            VaccineRecord[]
  Visit                    Visit[]

  @@index([estado])
  @@index([nombre])
  @@index([numeroFicha])
  @@index([ownerId])
}

model Prescription {
  id                  String              @id
  consultationId      String
  petId               String
  prescribedById      String
  prescribedAt        DateTime            @default(now())
  generalInstructions String?             @db.Text
  status              Prescription_status @default(PENDIENTE)
  createdAt           DateTime            @default(now())
  updatedAt           DateTime
  Dispense            Dispense[]
  Consultation        Consultation        @relation(fields: [consultationId], references: [id])
  Pet                 Pet                 @relation(fields: [petId], references: [id])
  User                User                @relation(fields: [prescribedById], references: [id])
  PrescriptionItem    PrescriptionItem[]

  @@index([consultationId])
  @@index([petId])
  @@index([prescribedById], map: "Prescription_prescribedById_fkey")
  @@index([status])
}

model PrescriptionItem {
  id             String       @id
  prescriptionId String
  name           String
  dosage         String
  frequency      String
  duration       String
  quantity       Int
  instructions   String?      @db.Text
  createdAt      DateTime     @default(now())
  Prescription   Prescription @relation(fields: [prescriptionId], references: [id], onDelete: Cascade)

  @@index([prescriptionId])
}

model PurchaseOrder {
  id                                    String               @id
  supplier                              String
  supplierContact                       String?
  totalAmount                           Decimal              @db.Decimal(10, 2)
  status                                PurchaseOrder_status @default(BORRADOR)
  createdById                           String
  sentAt                                DateTime?
  expectedDelivery                      DateTime?
  receivedAt                            DateTime?
  receivedById                          String?
  notes                                 String?              @db.Text
  createdAt                             DateTime             @default(now())
  updatedAt                             DateTime
  User_PurchaseOrder_createdByIdToUser  User                 @relation("PurchaseOrder_createdByIdToUser", fields: [createdById], references: [id])
  User_PurchaseOrder_receivedByIdToUser User?                @relation("PurchaseOrder_receivedByIdToUser", fields: [receivedById], references: [id])
  PurchaseOrderItem                     PurchaseOrderItem[]

  @@index([createdById], map: "PurchaseOrder_createdById_fkey")
  @@index([receivedById], map: "PurchaseOrder_receivedById_fkey")
  @@index([status])
  @@index([supplier])
}

model PurchaseOrderItem {
  id              String        @id
  purchaseOrderId String
  medicationId    String
  medicationName  String
  quantity        Int
  unitCost        Decimal       @db.Decimal(10, 2)
  subtotal        Decimal       @db.Decimal(10, 2)
  receivedQty     Int?
  createdAt       DateTime      @default(now())
  Medication      Medication    @relation(fields: [medicationId], references: [id])
  PurchaseOrder   PurchaseOrder @relation(fields: [purchaseOrderId], references: [id], onDelete: Cascade)

  @@index([medicationId])
  @@index([purchaseOrderId])
}

model SignosVitales {
  id                     String       @id
  consultationId         String
  registeredAt           DateTime     @default(now())
  temperatura            Float?
  frecuenciaCardiaca     Int?
  frecuenciaRespiratoria Int?
  peso                   Float?
  presionArterial        String?
  pulso                  Int?
  saturacionOxigeno      Float?
  glucosa                Float?
  hidratacion            String?
  mucosas                String?
  tiempoLlenadoCapilar   String?
  condicionCorporal      Int?
  escalaDolor            Int?
  localizacionDolor      String?
  observaciones          String?      @db.Text
  createdAt              DateTime     @default(now())
  Consultation           Consultation @relation(fields: [consultationId], references: [id], onDelete: Cascade)

  @@index([consultationId])
  @@index([registeredAt])
}

model StockAlert {
  id              String              @id
  medicationId    String
  type            StockAlert_type
  message         String              @db.Text
  priority        StockAlert_priority
  status          StockAlert_status   @default(ACTIVA)
  resolvedAt      DateTime?
  resolvedById    String?
  resolutionNotes String?             @db.Text
  createdAt       DateTime            @default(now())
  Medication      Medication          @relation(fields: [medicationId], references: [id])
  User            User?               @relation(fields: [resolvedById], references: [id])

  @@index([medicationId])
  @@index([resolvedById], map: "StockAlert_resolvedById_fkey")
  @@index([status, priority])
}

model StockMovement {
  id             String             @id
  medicationId   String
  type           StockMovement_type
  quantity       Int
  previousStock  Int
  newStock       Int
  reason         String?            @db.Text
  reference      String?
  batchNumber    String?
  expirationDate DateTime?
  performedById  String
  performedAt    DateTime           @default(now())
  Medication     Medication         @relation(fields: [medicationId], references: [id])
  User           User               @relation(fields: [performedById], references: [id])

  @@index([medicationId])
  @@index([performedAt])
  @@index([performedById], map: "StockMovement_performedById_fkey")
  @@index([type])
}

model Surgery {
  id                      String             @id
  petId                   String
  consultationId          String
  surgeonId               String
  type                    String
  scheduledDate           DateTime           @db.Date
  scheduledTime           String
  estimatedDuration       Int?
  status                  Surgery_status     @default(PROGRAMADA)
  prioridad               Surgery_prioridad  @default(MEDIA)
  preOpNotes              String?            @db.Text
  preOpStudies            String?            @db.LongText
  sedationAuthorized      Boolean            @default(false)
  consentSigned           Boolean            @default(false)
  consentSignedBy         String?
  consentSignedAt         DateTime?
  fastingConfirmed        Boolean            @default(false)
  startTime               DateTime?
  endTime                 DateTime?
  anesthesiaType          String?
  complications           String?            @db.Text
  postOpNotes             String?            @db.Text
  procedure               String?            @db.Text
  prognosis               Surgery_prognosis?
  postOpCare              String?            @db.Text
  hospitalizationRequired Boolean            @default(false)
  followUpDate            DateTime?
  createdAt               DateTime           @default(now())
  updatedAt               DateTime
  Hospitalization         Hospitalization?
  Consultation            Consultation       @relation(fields: [consultationId], references: [id])
  Pet                     Pet                @relation(fields: [petId], references: [id])
  User                    User               @relation(fields: [surgeonId], references: [id])

  @@index([consultationId], map: "Surgery_consultationId_fkey")
  @@index([petId])
  @@index([scheduledDate])
  @@index([status])
  @@index([surgeonId], map: "Surgery_surgeonId_fkey")
}

model Task {
  id                          String         @id
  titulo                      String
  descripcion                 String?        @db.Text
  tipo                        Task_tipo
  prioridad                   Task_prioridad
  estado                      Task_estado    @default(PENDIENTE)
  assigneeId                  String?
  createdById                 String
  petId                       String?
  visitId                     String?
  createdAt                   DateTime       @default(now())
  completedAt                 DateTime?
  User_Task_assigneeIdToUser  User?          @relation("Task_assigneeIdToUser", fields: [assigneeId], references: [id])
  User_Task_createdByIdToUser User           @relation("Task_createdByIdToUser", fields: [createdById], references: [id])
  Pet                         Pet?           @relation(fields: [petId], references: [id])
  Visit                       Visit?         @relation(fields: [visitId], references: [id])

  @@index([assigneeId, estado])
  @@index([createdById], map: "Task_createdById_fkey")
  @@index([petId], map: "Task_petId_fkey")
  @@index([tipo, prioridad])
  @@index([visitId], map: "Task_visitId_fkey")
}

model User {
  id                                             String            @id
  email                                          String            @unique
  password                                       String
  nombre                                         String
  rol                                            User_rol
  especialidad                                   String?
  telefono                                       String?
  activo                                         Boolean           @default(true)
  createdAt                                      DateTime          @default(now())
  updatedAt                                      DateTime
  Consultation                                   Consultation[]
  Dispense                                       Dispense[]
  Hospitalization                                Hospitalization[]
  LabRequest_LabRequest_completedByIdToUser      LabRequest[]      @relation("LabRequest_completedByIdToUser")
  LabRequest_LabRequest_requestedByIdToUser      LabRequest[]      @relation("LabRequest_requestedByIdToUser")
  MedicalNote                                    MedicalNote[]
  Monitoring                                     Monitoring[]
  Notification                                   Notification[]
  Prescription                                   Prescription[]
  PurchaseOrder_PurchaseOrder_createdByIdToUser  PurchaseOrder[]   @relation("PurchaseOrder_createdByIdToUser")
  PurchaseOrder_PurchaseOrder_receivedByIdToUser PurchaseOrder[]   @relation("PurchaseOrder_receivedByIdToUser")
  StockAlert                                     StockAlert[]
  StockMovement                                  StockMovement[]
  Surgery                                        Surgery[]
  Task_Task_assigneeIdToUser                     Task[]            @relation("Task_assigneeIdToUser")
  Task_Task_createdByIdToUser                    Task[]            @relation("Task_createdByIdToUser")

  @@index([email])
  @@index([rol])
}

model VaccineRecord {
  id           String    @id
  petId        String
  nombre       String
  fecha        DateTime
  proximaDosis DateTime?
  lote         String?
  aplicadaPor  String?
  notas        String?   @db.Text
  createdAt    DateTime  @default(now())
  Pet          Pet       @relation(fields: [petId], references: [id], onDelete: Cascade)

  @@index([petId, proximaDosis])
}

model Visit {
  id               String            @id
  petId            String
  arrivalTime      DateTime          @default(now())
  status           Visit_status      @default(RECIEN_LLEGADO)
  tipoVisita       Visit_tipoVisita?
  motivo           String?           @db.Text
  prioridad        Visit_prioridad?
  peso             Float?
  temperatura      Float?
  primeraVisita    Boolean           @default(false)
  antecedentes     String?           @db.Text
  assignedDoctorId String?
  dischargeNotes   String?           @db.Text
  dischargedAt     DateTime?
  createdAt        DateTime          @default(now())
  updatedAt        DateTime
  Consultation     Consultation?
  Payment          Payment?
  Task             Task[]
  Pet              Pet               @relation(fields: [petId], references: [id])

  @@index([arrivalTime])
  @@index([petId])
  @@index([status])
}

enum StockMovement_type {
  ENTRADA
  SALIDA
  AJUSTE
  DEVOLUCION
  VENCIDO
  MERMA
}

enum Notification_tipo {
  NUEVO_PACIENTE
  TRIAGE_COMPLETADO
  ESTUDIOS_SOLICITADOS
  RESULTADOS_LISTOS
  RECETA_PENDIENTE
  MEDICAMENTOS_LISTOS
  CIRUGIA_PROGRAMADA
  ALTA_PENDIENTE
  STOCK_BAJO
  CITA_AGENDADA_MEDICO
  GENERAL
}

enum StockAlert_type {
  STOCK_BAJO
  AGOTADO
  POR_VENCER
  VENCIDO
}

enum Visit_status {
  RECIEN_LLEGADO
  EN_ESPERA
  EN_CONSULTA
  EN_ESTUDIOS
  EN_FARMACIA
  CIRUGIA_PROGRAMADA
  EN_CIRUGIA
  HOSPITALIZADO
  LISTO_PARA_ALTA
  ALTA
}

enum Medication_category {
  ANTIBIOTICO
  ANALGESICO
  ANTIINFLAMATORIO
  ANTIPARASITARIO
  VACUNA
  VITAMINA
  SUERO
  ANESTESICO
  DERMATOLOGICO
  OFTALMICO
  CARDIACO
  HORMONAL
  OTRO
}

enum Payment_metodoPago {
  EFECTIVO
  TARJETA
  TRANSFERENCIA
}

enum Task_tipo {
  TRIAGE
  CONSULTA
  LABORATORIO
  FARMACIA
  CIRUGIA
  ALTA
  GENERAL
}

enum Visit_tipoVisita {
  CONSULTA_GENERAL
  SEGUIMIENTO
  MEDICINA_PREVENTIVA
  EMERGENCIA
}

enum User_rol {
  ADMIN
  RECEPCION
  MEDICO
  LABORATORIO
  FARMACIA
}

enum Task_prioridad {
  ALTA
  MEDIA
  BAJA
}

enum Pet_especie {
  PERRO
  GATO
  AVE
  ROEDOR
  REPTIL
  OTRO
}

enum MedicalNote_type {
  EVOLUCION
  INTERCONSULTA
  ORDEN
  GENERAL
}

enum PurchaseOrder_status {
  BORRADOR
  PENDIENTE
  ENVIADA
  PARCIAL
  RECIBIDA
  CANCELADA
}

enum Appointment_tipo {
  CONSULTA_GENERAL
  SEGUIMIENTO
  VACUNACION
  CIRUGIA
  EMERGENCIA
}

enum StockAlert_priority {
  ALTA
  MEDIA
  BAJA
}

enum Dispense_status {
  PENDIENTE
  COMPLETO
  PARCIAL
}

enum LabRequest_type {
  HEMOGRAMA
