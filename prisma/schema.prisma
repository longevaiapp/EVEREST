generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id                     String            @id @default(cuid())
  email                  String            @unique
  password               String
  nombre                 String
  rol                    UserRole
  especialidad           String?
  telefono               String?
  activo                 Boolean           @default(true)
  createdAt              DateTime          @default(now())
  updatedAt              DateTime          @updatedAt
  consultations          Consultation[]
  dispenses              Dispense[]
  hospitalizations       Hospitalization[]
  labRequestsDone        LabRequest[]      @relation("LabCompletedBy")
  labRequestsMade        LabRequest[]      @relation("LabRequestedBy")
  medicalNotes           MedicalNote[]
  monitorings            Monitoring[]
  notifications          Notification[]
  prescriptions          Prescription[]
  purchaseOrdersCreated  PurchaseOrder[]   @relation("POCreator")
  purchaseOrdersReceived PurchaseOrder[]   @relation("POReceiver")
  alertsResolved         StockAlert[]
  stockMovements         StockMovement[]
  surgeriesLead          Surgery[]         @relation("SurgeonLead")
  tasksAssigned          Task[]            @relation("TaskAssignee")
  tasksCreated           Task[]            @relation("TaskCreator")

  @@index([email])
  @@index([rol])
}

model Notification {
  id        String           @id @default(cuid())
  userId    String
  tipo      NotificationType
  titulo    String
  mensaje   String           @db.Text
  leida     Boolean          @default(false)
  data      String?          @db.LongText
  createdAt DateTime         @default(now())
  user      User             @relation(fields: [userId], references: [id])

  @@index([userId, leida])
  @@index([createdAt])
}

model Task {
  id          String     @id @default(cuid())
  titulo      String
  descripcion String?    @db.Text
  tipo        TaskType
  prioridad   Priority
  estado      TaskStatus @default(PENDIENTE)
  assigneeId  String?
  createdById String
  petId       String?
  visitId     String?
  createdAt   DateTime   @default(now())
  completedAt DateTime?
  assignee    User?      @relation("TaskAssignee", fields: [assigneeId], references: [id])
  createdBy   User       @relation("TaskCreator", fields: [createdById], references: [id])
  pet         Pet?       @relation(fields: [petId], references: [id])
  visit       Visit?     @relation(fields: [visitId], references: [id])

  @@index([assigneeId, estado])
  @@index([tipo, prioridad])
  @@index([createdById], map: "Task_createdById_fkey")
  @@index([petId], map: "Task_petId_fkey")
  @@index([visitId], map: "Task_visitId_fkey")
}

model Owner {
  id           String   @id @default(cuid())
  nombre       String
  telefono     String   @unique
  email        String?
  direccion    String?  @db.Text
  ciudad       String?
  codigoPostal String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  pets         Pet[]

  @@index([telefono])
  @@index([nombre])
}

model Pet {
  id                       String            @id @default(cuid())
  numeroFicha              String            @unique
  ownerId                  String
  nombre                   String
  especie                  Species
  raza                     String?
  sexo                     Sexo
  fechaNacimiento          DateTime?
  peso                     Float?
  color                    String?
  condicionCorporal        BodyCondition?
  fotoUrl                  String?           @db.LongText
  snapTest                 String?           @db.Text
  analisisClinicos         String?           @db.Text
  antecedentes             String?           @db.Text
  desparasitacionExterna   Boolean?
  ultimaDesparasitacion    DateTime?
  vacunasTexto             String?           @db.Text
  vacunasActualizadas      Boolean?
  ultimaVacuna             DateTime?
  esterilizado             Boolean?
  otrasCirugias            Boolean?
  detalleCirugias          String?           @db.Text
  ultimoCelo               DateTime?
  cantidadPartos           Int?
  ultimoParto              DateTime?
  alimento                 String?
  porcionesPorDia          String?
  otrosAlimentos           String?
  frecuenciaOtrosAlimentos String?
  alergias                 String?           @db.Text
  enfermedadesCronicas     String?           @db.Text
  conviveOtrasMascotas     Boolean?
  cualesMascotas           String?
  actividadFisica          Boolean?
  frecuenciaActividad      String?
  saleViaPublica           Boolean?
  frecuenciaSalida         String?
  otrosDatos               String?           @db.Text
  estado                   PatientStatus     @default(RECIEN_LLEGADO)
  fechaIngreso             DateTime          @default(now())
  primeraVisita            Boolean           @default(true)
  activo                   Boolean           @default(true)
  createdAt                DateTime          @default(now())
  updatedAt                DateTime          @updatedAt
  appointments             Appointment[]
  consultations            Consultation[]
  dispenses                Dispense[]
  hospitalizations         Hospitalization[]
  labRequests              LabRequest[]
  medicalNotes             MedicalNote[]
  owner                    Owner             @relation(fields: [ownerId], references: [id], onDelete: Cascade)
  prescriptions            Prescription[]
  surgeries                Surgery[]
  tasks                    Task[]
  vaccineRecords           VaccineRecord[]
  visits                   Visit[]

  @@index([ownerId])
  @@index([numeroFicha])
  @@index([nombre])
  @@index([estado])
}

model VaccineRecord {
  id           String    @id @default(cuid())
  petId        String
  nombre       String
  fecha        DateTime
  proximaDosis DateTime?
  lote         String?
  aplicadaPor  String?
  notas        String?   @db.Text
  createdAt    DateTime  @default(now())
  pet          Pet       @relation(fields: [petId], references: [id], onDelete: Cascade)

  @@index([petId, proximaDosis])
}

model Visit {
  id               String        @id @default(cuid())
  petId            String
  arrivalTime      DateTime      @default(now())
  status           VisitStatus   @default(RECIEN_LLEGADO)
  tipoVisita       VisitType?
  motivo           String?       @db.Text
  prioridad        Priority?
  peso             Float?
  temperatura      Float?
  primeraVisita    Boolean       @default(false)
  antecedentes     String?       @db.Text
  assignedDoctorId String?
  dischargeNotes   String?       @db.Text
  dischargedAt     DateTime?
  createdAt        DateTime      @default(now())
  updatedAt        DateTime      @updatedAt
  consultation     Consultation?
  payment          Payment?
  tasks            Task[]
  pet              Pet           @relation(fields: [petId], references: [id])

  @@index([petId])
  @@index([status])
  @@index([arrivalTime])
}

model Appointment {
  id          String            @id @default(cuid())
  petId       String
  fecha       DateTime          @db.Date
  hora        String
  tipo        AppointmentType
  motivo      String            @db.Text
  confirmada  Boolean           @default(false)
  cancelada   Boolean           @default(false)
  notas       String?           @db.Text
  createdById String?
  createdAt   DateTime          @default(now())
  updatedAt   DateTime          @updatedAt
  status      AppointmentStatus @default(PENDIENTE)
  visitId     String?
  pet         Pet               @relation(fields: [petId], references: [id])

  @@index([fecha])
  @@index([petId])
  @@index([status])
}

model Payment {
  id          String        @id @default(cuid())
  visitId     String        @unique
  total       Decimal       @db.Decimal(10, 2)
  metodoPago  PaymentMethod
  fecha       DateTime      @default(now())
  recibidoPor String?
  notas       String?       @db.Text
  createdAt   DateTime      @default(now())
  visit       Visit         @relation(fields: [visitId], references: [id])

  @@index([fecha])
}

model Consultation {
  id                   String             @id @default(cuid())
  visitId              String             @unique
  petId                String
  doctorId             String
  startTime            DateTime           @default(now())
  endTime              DateTime?
  symptoms             String?            @db.Text
  physicalExam         String?            @db.Text
  diagnosis            String?            @db.Text
  treatment            String?            @db.Text
  notes                String?            @db.Text
  vitalTemperature     Float?
  vitalHeartRate       Int?
  vitalRespiratoryRate Int?
  vitalWeight          Float?
  followUpRequired     Boolean            @default(false)
  followUpDate         DateTime?
  status               ConsultationStatus @default(EN_PROGRESO)
  createdAt            DateTime           @default(now())
  updatedAt            DateTime           @updatedAt
  duration             Int?
  followUpNotes        String?            @db.Text
  historiaEnfermedad   String?            @db.Text
  motivoConsulta       String?            @db.Text
  soapAssessment       String?            @db.Text
  soapObjective        String?            @db.Text
  soapPlan             String?            @db.Text
  soapSubjective       String?            @db.Text
  vitalBloodPressure   String?
  vitalHydration       String?
  vitalMucosas         String?
  vitalPulse           Int?
  doctor               User               @relation(fields: [doctorId], references: [id])
  pet                  Pet                @relation(fields: [petId], references: [id])
  visit                Visit              @relation(fields: [visitId], references: [id])
  diagnosticos         Diagnostico[]
  hospitalization      Hospitalization?
  labRequests          LabRequest[]
  prescriptions        Prescription[]
  signosVitales        SignosVitales[]
  surgeries            Surgery[]

  @@index([petId])
  @@index([doctorId])
  @@index([startTime])
  @@index([status])
}

model Diagnostico {
  id             String          @id @default(cuid())
  consultationId String
  codigoCIE10    String?
  nombreCIE10    String?
  descripcion    String          @db.Text
  tipo           DiagnosticoTipo @default(PRESUNTIVO)
  severidad      Severidad?
  observaciones  String?         @db.Text
  diferenciales  String?         @db.Text
  orden          Int             @default(1)
  esPrincipal    Boolean         @default(false)
  createdAt      DateTime        @default(now())
  updatedAt      DateTime        @updatedAt
  consultation   Consultation    @relation(fields: [consultationId], references: [id], onDelete: Cascade)

  @@index([consultationId])
  @@index([codigoCIE10])
}

model SignosVitales {
  id                     String       @id @default(cuid())
  consultationId         String
  registeredAt           DateTime     @default(now())
  temperatura            Float?
  frecuenciaCardiaca     Int?
  frecuenciaRespiratoria Int?
  peso                   Float?
  presionArterial        String?
  pulso                  Int?
  saturacionOxigeno      Float?
  glucosa                Float?
  hidratacion            String?
  mucosas                String?
  tiempoLlenadoCapilar   String?
  condicionCorporal      Int?
  escalaDolor            Int?
  localizacionDolor      String?
  observaciones          String?      @db.Text
  createdAt              DateTime     @default(now())
  consultation           Consultation @relation(fields: [consultationId], references: [id], onDelete: Cascade)

  @@index([consultationId])
  @@index([registeredAt])
}

model LabRequest {
  id             String           @id @default(cuid())
  consultationId String
  petId          String
  requestedById  String
  requestedAt    DateTime         @default(now())
  type           LabType
  urgency        LabUrgency       @default(NORMAL)
  notes          String?          @db.Text
  status         LabRequestStatus @default(PENDIENTE)
  results        String?          @db.Text
  resultFiles    String?          @db.LongText
  completedAt    DateTime?
  completedById  String?
  createdAt      DateTime         @default(now())
  updatedAt      DateTime         @updatedAt
  completedBy    User?            @relation("LabCompletedBy", fields: [completedById], references: [id])
  consultation   Consultation     @relation(fields: [consultationId], references: [id])
  pet            Pet              @relation(fields: [petId], references: [id])
  requestedBy    User             @relation("LabRequestedBy", fields: [requestedById], references: [id])

  @@index([petId])
  @@index([status])
  @@index([consultationId])
  @@index([completedById], map: "LabRequest_completedById_fkey")
  @@index([requestedById], map: "LabRequest_requestedById_fkey")
}

model Prescription {
  id                  String             @id @default(cuid())
  consultationId      String
  petId               String
  prescribedById      String
  prescribedAt        DateTime           @default(now())
  generalInstructions String?            @db.Text
  status              PrescriptionStatus @default(PENDIENTE)
  createdAt           DateTime           @default(now())
  updatedAt           DateTime           @updatedAt
  dispenses           Dispense[]
  consultation        Consultation       @relation(fields: [consultationId], references: [id])
  pet                 Pet                @relation(fields: [petId], references: [id])
  prescribedBy        User               @relation(fields: [prescribedById], references: [id])
  items               PrescriptionItem[]

  @@index([petId])
  @@index([status])
  @@index([consultationId])
  @@index([prescribedById], map: "Prescription_prescribedById_fkey")
}

model PrescriptionItem {
  id             String       @id @default(cuid())
  prescriptionId String
  name           String
  dosage         String
  frequency      String
  duration       String
  quantity       Int
  instructions   String?      @db.Text
  createdAt      DateTime     @default(now())
  prescription   Prescription @relation(fields: [prescriptionId], references: [id], onDelete: Cascade)

  @@index([prescriptionId])
}

model Surgery {
  id                      String           @id @default(cuid())
  petId                   String
  consultationId          String
  surgeonId               String
  type                    String
  scheduledDate           DateTime         @db.Date
  scheduledTime           String
  estimatedDuration       Int?
  status                  SurgeryStatus    @default(PROGRAMADA)
  prioridad               Priority         @default(MEDIA)
  preOpNotes              String?          @db.Text
  preOpStudies            String?          @db.LongText
  sedationAuthorized      Boolean          @default(false)
  consentSigned           Boolean          @default(false)
  consentSignedBy         String?
  consentSignedAt         DateTime?
  fastingConfirmed        Boolean          @default(false)
  startTime               DateTime?
  endTime                 DateTime?
  anesthesiaType          String?
  complications           String?          @db.Text
  postOpNotes             String?          @db.Text
  procedure               String?          @db.Text
  prognosis               Prognosis?
  postOpCare              String?          @db.Text
  hospitalizationRequired Boolean          @default(false)
  followUpDate            DateTime?
  createdAt               DateTime         @default(now())
  updatedAt               DateTime         @updatedAt
  hospitalization         Hospitalization?
  consultation            Consultation     @relation(fields: [consultationId], references: [id])
  pet                     Pet              @relation(fields: [petId], references: [id])
  surgeon                 User             @relation("SurgeonLead", fields: [surgeonId], references: [id])

  @@index([petId])
  @@index([scheduledDate])
  @@index([status])
  @@index([consultationId], map: "Surgery_consultationId_fkey")
  @@index([surgeonId], map: "Surgery_surgeonId_fkey")
}

model Hospitalization {
  id                  String                @id @default(cuid())
  petId               String
  consultationId      String                @unique
  surgeryId           String?               @unique
  admittedById        String
  admittedAt          DateTime              @default(now())
  dischargedAt        DateTime?
  reason              String                @db.Text
  location            String?
  status              HospitalizationStatus @default(ACTIVA)
  createdAt           DateTime              @default(now())
  updatedAt           DateTime              @updatedAt
  cuidadosEspeciales  String?               @db.Text
  dischargeNotes      String?               @db.Text
  estimacionDias      Int?
  frecuenciaMonitoreo String?
  admittedBy          User                  @relation(fields: [admittedById], references: [id])
  consultation        Consultation          @relation(fields: [consultationId], references: [id])
  pet                 Pet                   @relation(fields: [petId], references: [id])
  surgery             Surgery?              @relation(fields: [surgeryId], references: [id])
  monitorings         Monitoring[]

  @@index([petId])
  @@index([status])
  @@index([admittedById], map: "Hospitalization_admittedById_fkey")
}

model Monitoring {
  id                     String              @id @default(cuid())
  hospitalizationId      String
  recordedById           String
  recordedAt             DateTime            @default(now())
  temperatura            Float?
  frecuenciaCardiaca     Int?
  frecuenciaRespiratoria Int?
  presionArterial        String?
  nivelConciencia        ConsciousnessLevel?
  escalaDolor            Int?
  observaciones          String?             @db.Text
  createdAt              DateTime            @default(now())
  hospitalization        Hospitalization     @relation(fields: [hospitalizationId], references: [id], onDelete: Cascade)
  recordedBy             User                @relation(fields: [recordedById], references: [id])

  @@index([hospitalizationId])
  @@index([recordedAt])
  @@index([recordedById], map: "Monitoring_recordedById_fkey")
}

model MedicalNote {
  id             String          @id @default(cuid())
  petId          String
  consultationId String?
  createdById    String
  type           MedicalNoteType
  content        String          @db.Text
  isPrivate      Boolean         @default(false)
  createdAt      DateTime        @default(now())
  updatedAt      DateTime        @updatedAt
  createdBy      User            @relation(fields: [createdById], references: [id])
  pet            Pet             @relation(fields: [petId], references: [id])

  @@index([petId])
  @@index([type])
  @@index([createdById], map: "MedicalNote_createdById_fkey")
}

model Medication {
  id                    String              @id @default(cuid())
  name                  String
  genericName           String?
  category              MedicationCategory
  presentation          String
  concentration         String?
  unit                  String
  currentStock          Int                 @default(0)
  minStock              Int
  maxStock              Int?
  location              String?
  requiresRefrigeration Boolean             @default(false)
  isControlled          Boolean             @default(false)
  costPrice             Decimal?            @db.Decimal(10, 2)
  salePrice             Decimal             @db.Decimal(10, 2)
  expirationDate        DateTime?
  lastRestocked         DateTime?
  supplier              String?
  supplierCode          String?
  activo                Boolean             @default(true)
  createdAt             DateTime            @default(now())
  updatedAt             DateTime            @updatedAt
  imageUrl              String?             @db.LongText
  dispenseItems         DispenseItem[]
  purchaseOrderItems    PurchaseOrderItem[]
  stockAlerts           StockAlert[]
  stockMovements        StockMovement[]

  @@index([name])
  @@index([category])
  @@index([currentStock])
}

model Dispense {
  id             String         @id @default(cuid())
  prescriptionId String
  petId          String
  dispensedById  String
  dispensedAt    DateTime       @default(now())
  status         DispenseStatus @default(PENDIENTE)
  notes          String?        @db.Text
  deliveredTo    String?
  signature      String?        @db.LongText
  createdAt      DateTime       @default(now())
  dispensedBy    User           @relation(fields: [dispensedById], references: [id])
  pet            Pet            @relation(fields: [petId], references: [id])
  prescription   Prescription   @relation(fields: [prescriptionId], references: [id])
  items          DispenseItem[]

  @@index([prescriptionId])
  @@index([petId])
  @@index([status])
  @@index([dispensedById], map: "Dispense_dispensedById_fkey")
}

model DispenseItem {
  id             String     @id @default(cuid())
  dispenseId     String
  medicationId   String
  medicationName String
  requestedQty   Int
  dispensedQty   Int
  reason         String?
  unitPrice      Decimal    @db.Decimal(10, 2)
  subtotal       Decimal    @db.Decimal(10, 2)
  createdAt      DateTime   @default(now())
  dispense       Dispense   @relation(fields: [dispenseId], references: [id], onDelete: Cascade)
  medication     Medication @relation(fields: [medicationId], references: [id])

  @@index([dispenseId])
  @@index([medicationId])
}

model StockMovement {
  id             String       @id @default(cuid())
  medicationId   String
  type           MovementType
  quantity       Int
  previousStock  Int
  newStock       Int
  reason         String?      @db.Text
  reference      String?
  batchNumber    String?
  expirationDate DateTime?
  performedById  String
  performedAt    DateTime     @default(now())
  medication     Medication   @relation(fields: [medicationId], references: [id])
  performedBy    User         @relation(fields: [performedById], references: [id])

  @@index([medicationId])
  @@index([type])
  @@index([performedAt])
  @@index([performedById], map: "StockMovement_performedById_fkey")
}

model StockAlert {
  id              String      @id @default(cuid())
  medicationId    String
  type            AlertType
  message         String      @db.Text
  priority        Priority
  status          AlertStatus @default(ACTIVA)
  resolvedAt      DateTime?
  resolvedById    String?
  resolutionNotes String?     @db.Text
  createdAt       DateTime    @default(now())
  medication      Medication  @relation(fields: [medicationId], references: [id])
  resolvedBy      User?       @relation(fields: [resolvedById], references: [id])

  @@index([medicationId])
  @@index([status, priority])
  @@index([resolvedById], map: "StockAlert_resolvedById_fkey")
}

model PurchaseOrder {
  id               String              @id @default(cuid())
  supplier         String
  supplierContact  String?
  totalAmount      Decimal             @db.Decimal(10, 2)
  status           PurchaseOrderStatus @default(BORRADOR)
  createdById      String
  sentAt           DateTime?
  expectedDelivery DateTime?
  receivedAt       DateTime?
  receivedById     String?
  notes            String?             @db.Text
  createdAt        DateTime            @default(now())
  updatedAt        DateTime            @updatedAt
  createdBy        User                @relation("POCreator", fields: [createdById], references: [id])
  receivedBy       User?               @relation("POReceiver", fields: [receivedById], references: [id])
  items            PurchaseOrderItem[]

  @@index([status])
  @@index([supplier])
  @@index([createdById], map: "PurchaseOrder_createdById_fkey")
  @@index([receivedById], map: "PurchaseOrder_receivedById_fkey")
}

model PurchaseOrderItem {
  id              String        @id @default(cuid())
  purchaseOrderId String
  medicationId    String
  medicationName  String
  quantity        Int
  unitCost        Decimal       @db.Decimal(10, 2)
  subtotal        Decimal       @db.Decimal(10, 2)
  receivedQty     Int?
  createdAt       DateTime      @default(now())
  medication      Medication    @relation(fields: [medicationId], references: [id])
  purchaseOrder   PurchaseOrder @relation(fields: [purchaseOrderId], references: [id], onDelete: Cascade)

  @@index([purchaseOrderId])
  @@index([medicationId])
}

enum UserRole {
  ADMIN
  RECEPCION
  MEDICO
  LABORATORIO
  FARMACIA
}

enum NotificationType {
  NUEVO_PACIENTE
  TRIAGE_COMPLETADO
  ESTUDIOS_SOLICITADOS
  RESULTADOS_LISTOS
  RECETA_PENDIENTE
  MEDICAMENTOS_LISTOS
  CIRUGIA_PROGRAMADA
  ALTA_PENDIENTE
  STOCK_BAJO
  GENERAL
}

enum TaskType {
  TRIAGE
  CONSULTA
  LABORATORIO
  FARMACIA
  CIRUGIA
  ALTA
  GENERAL
}

enum TaskStatus {
  PENDIENTE
  EN_PROGRESO
  COMPLETADA
  CANCELADA
}

enum Priority {
  ALTA
  MEDIA
  BAJA
}

enum Species {
  PERRO
  GATO
  AVE
  ROEDOR
  REPTIL
  OTRO
}

enum Sexo {
  MACHO
  HEMBRA
}

enum BodyCondition {
  MUY_DELGADO
  DELGADO
  IDEAL
  SOBREPESO
  OBESO
}

enum PatientStatus {
  RECIEN_LLEGADO
  EN_ESPERA
  EN_CONSULTA
  EN_ESTUDIOS
  EN_FARMACIA
  CIRUGIA_PROGRAMADA
  EN_CIRUGIA
  HOSPITALIZADO
  LISTO_PARA_ALTA
  ALTA
}

enum VisitStatus {
  RECIEN_LLEGADO
  EN_ESPERA
  EN_CONSULTA
  EN_ESTUDIOS
  EN_FARMACIA
  CIRUGIA_PROGRAMADA
  EN_CIRUGIA
  HOSPITALIZADO
  LISTO_PARA_ALTA
  ALTA
}

enum VisitType {
  CONSULTA_GENERAL
  SEGUIMIENTO
  MEDICINA_PREVENTIVA
  EMERGENCIA
}

enum AppointmentStatus {
  PENDIENTE
  CONFIRMADA
  EN_CONSULTA
  COMPLETADA
  NO_ASISTIO
  CANCELADA
}

enum AppointmentType {
  CONSULTA_GENERAL
  SEGUIMIENTO
  VACUNACION
  CIRUGIA
  EMERGENCIA
}

enum PaymentMethod {
  EFECTIVO
  TARJETA
  TRANSFERENCIA
}

enum ConsultationStatus {
  EN_PROGRESO
  COMPLETADA
}

enum DiagnosticoTipo {
  PRESUNTIVO
  DEFINITIVO
  DIFERENCIAL
}

enum Severidad {
  LEVE
  MODERADO
  SEVERO
  CRITICO
}

enum LabType {
  HEMOGRAMA
  QUIMICA_SANGUINEA
  URINALISIS
  RAYOS_X
  ULTRASONIDO
  ELECTROCARDIOGRAMA
  CITOLOGIA
  BIOPSIA
  COPROLOGIA
  PERFIL_TIROIDEO
}

enum LabUrgency {
  NORMAL
  URGENTE
}

enum LabRequestStatus {
